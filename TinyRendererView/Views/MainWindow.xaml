<Window x:Class="TinyRendererView.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TinyRendererView"
        mc:Ignorable="d"
        Title="MainWindow" Height="451.202" Width="701.403">
    <Grid Margin="0,0,-6.333,0">
        <Image x:Name="Chart" Margin="0,0,168.667,0"/>
        <Button x:Name="button_Load" Content="Load" HorizontalAlignment="Left" Margin="543,41,0,0" VerticalAlignment="Top" Width="75" Height="27" Click="button_Load_Click"/>
        <Label x:Name="label_filePath" Content="Please load an OBJ model" HorizontalAlignment="Left" Margin="543,10,-0.333,0" VerticalAlignment="Top" Width="152"/>
        <Button x:Name="stepButton" Content="Next frame" HorizontalAlignment="Left" Margin="543,186,0,0" VerticalAlignment="Top" Width="75" Click="stepButton_Click"/>
        <CheckBox x:Name="Paused" Content="Paused" HorizontalAlignment="Left" Margin="543,112,0,0" VerticalAlignment="Top" Checked="pausedCheckbox_Checked" Unchecked="pausedCheckbox_Unchecked"/>
    </Grid>
</Window>
